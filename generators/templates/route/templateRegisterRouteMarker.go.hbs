		routesConstants.{{pascalCase requestType}}{{pascalCase fileName}}RouteConst,
		{{camelCase model}}Routes.jwt.IsAuthorizedMiddleware(),
		{{camelCase model}}Routes.{{camelCase model}}Handlers[{{requestType}}{{pascalCase model}}{{pascalCase fileName}}Const].Handle,
	)
